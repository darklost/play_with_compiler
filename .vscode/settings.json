{
    "editor.snippetSuggestions": "bottom",
    "luaide.scriptRoots": [
        "${workspaceRoot}/craft",
        "${workspaceRoot}/craft/lexer",
    ],
    "luaide.openUriConfig": [

        {
            "name": "百度搜索",
            "url": "https://www.baidu.com/s?wd={selection}"
        },
        {
            "name": "百度翻译",
            "url": "http://fanyi.baidu.com/#zh/en/{selection}"
        }
    ],
    
    "luaide.apiType": "none",
    "luaide.ignoreDir": [
        
    ],
    "luaide.requireFunNames": [
        "require",
        "import"
    ],
    "luaide.tagIsShowFileName":true,
    "luaide.ctorNames": [
        "ctor"
    ],
    "luaide.automaticDownloadingDebugFile": false,
    "java.project.outputPath": "playscript-java\\out",
    "java.project.referencedLibraries": [
        "lib/**/*.jar",
        "playscript-java\\lib\\antlr-4.7.2-complete.jar",
        "playscript-java\\lib\\asm-7.2.jar",
        "playscript-java\\lib\\asm-analysis-7.2.jar",
        "playscript-java\\lib\\asm-commons-7.2.jar",
        "playscript-java\\lib\\asm-tree-7.2.jar",
        "playscript-java\\lib\\asm-util-7.2.jar"
    ]
}